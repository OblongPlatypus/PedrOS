os.loadAPI("OS/API/draw")
rednet.open("top")

draw.inputBox("Select Turtle:")
input = read()
tId = tonumber(input)
draw.inputBox("Name Program:")
input = read()
pName = tostring(input)
draw.inputBox("Choose Program:")
input = read()
sData = tostring(input)

rednet.send(tId, "batch")
sleep(0.5)
rednet.send(tId, pName)
sleep(0.5)
data = fs.open(sData, "r")
dump = data.readAll()
data.close()
rednet.send(tId, dump)
